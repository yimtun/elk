wget  -P /opt/  https://artifacts.elastic.co/downloads/kibana/kibana-5.6.3-linux-x86_64.tar.gz

tar xzvf /opt/kibana-5.6.3-linux-x86_64.tar.gz    -C /opt/


echo 'server.host: "172.16.99.2"'  >>  /opt/kibana-5.6.3-linux-x86_64/config/kibana.yml

/opt/kibana-5.6.3-linux-x86_64/bin/kibana

